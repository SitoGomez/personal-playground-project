{
  "name": "personal-playground-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start --entryFile app/main",
    "start:dev": "NODE_ENV=development nest start --entryFile app/main --watch",
    "start:debug": "NODE_ENV=development nest start --entryFile app/main --debug 0.0.0.0:9229 --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "mikroorm:auth:db:commands:create-migration": "pnpm mikro-orm migration:create --blank --config src/auth/shared/infrastructure/databases/mikroOrm/MikroOrmCommandsDDBB.cli.config.ts",
    "mikroorm:auth:db:commands:rollback-last-migration": "pnpm mikro-orm migration:down --config src/auth/shared/infrastructure/databases/mikroOrm/MikroOrmCommandsDDBB.cli.config.ts",
    "mikroorm:auth:db:commands:recreate-ddbb": "pnpm mikro-orm migration:fresh  --config src/auth/shared/infrastructure/databases/mikroOrm/MikroOrmCommandsDDBB.cli.config.ts",
    "mikroorm:auth:db:commands:users:seed": "pnpm mikro-orm seeder:run --config src/auth/shared/infrastructure/databases/mikroOrm/MikroOrmCommandsDDBB.cli.config.ts --class=MikroOrmUserSeeder",
    "mikroorm:analytics:db:queries:create-migration": "pnpm mikro-orm migration:create --blank --config src/analytics/shared/infrastructure/databases/mikroOrm/MikroOrmQueriesDDBB.cli.config.ts",
    "mikroorm:analytics:db:queries:rollback-last-migration": "pnpm mikro-orm migration:down --config src/analytics/shared/infrastructure/databases/mikroOrm/MikroOrmQueriesDDBB.cli.config.ts",
    "mikroorm:analytics:db:queries:recreate-ddbb": "pnpm mikro-orm migration:fresh  --config src/analytics/shared/infrastructure/databases/mikroOrm/MikroOrmQueriesDDBB.cli.config.ts",
    "mikroorm:analytics:db:queries:users-activity:seed": "pnpm mikro-orm seeder:run --config src/analytics/shared/infrastructure/databases/mikroOrm/MikroOrmQueriesDDBB.cli.config.ts --class=MikroOrmUserActivitySeeder",
    "prepare": "husky"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.4.16",
    "@mikro-orm/migrations": "^6.4.16",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.4.16",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "colorette": "^2.0.20",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.5.0",
    "prom-client": "^15.1.3",
    "rabbitmq-client": "^5.0.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.8.0",
    "@mikro-orm/cli": "^6.4.16",
    "@mikro-orm/reflection": "^6.4.16",
    "@mikro-orm/seeder": "^6.4.16",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
